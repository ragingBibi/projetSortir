<form method="post" action="{{ path('app_venue_delete', {'id': venue.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce lieu?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ venue.id) }}">
    <button class="btn btn-primary">Supprimer</button>
</form>
